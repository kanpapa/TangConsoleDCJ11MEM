// | 3V3 | GND | NC(~WP) |  ~DET   | NC(D2)  |  NC(D1) |
// +-----+-----+---------+---------+---------+---------+
//
// MISO(D0) should be pulled up 10k to 3V3
IO_LOC "sd2_cs_n"  V18; IO_PORT "sd2_cs_n"  PULL_MODE=NONE IO_TYPE=LVCMOS33;
IO_LOC "sd2_mosi"  G21; IO_PORT "sd2_mosi"  PULL_MODE=NONE IO_TYPE=LVCMOS33;
IO_LOC "sd2_miso"  F18; IO_PORT "sd2_miso"  PULL_MODE=UP   IO_TYPE=LVCMOS33;
IO_LOC "sd2_det_n" E18; IO_PORT "sd2_det_n" PULL_MODE=NONE IO_TYPE=LVCMOS33;
IO_LOC "sd2_clk"   C22; IO_PORT "sd2_clk"   PULL_MODE=NONE IO_TYPE=LVCMOS33;

