//
// Pinout for TangConsoleDCJ11MEM rev.2 board
//
// clock (50MHz clock)
// 
IO_LOC "sys_clk50" V22; IO_PORT "sys_clk50" PULL_MODE=NONE IO_TYPE=LVCMOS33 DRIVE=OFF;

//
// SW (on-board user keys)
//
IO_LOC "sw1_n"  AA13; IO_PORT "sw1_n" PULL_MODE=UP IO_TYPE=LVCMOS15;
IO_LOC "sw2_n"  AB13; IO_PORT "sw2_n" PULL_MODE=UP IO_TYPE=LVCMOS15;

//
// LED (on-board LEDs)
//
IO_LOC "led_r"  U12; IO_PORT "led_r"  PULL_MODE=NONE IO_TYPE=LVCMOS33; // READY
IO_LOC "led_b"  G11; IO_PORT "led_b"  PULL_MODE=NONE IO_TYPE=LVCMOS33; // DONE

//
// FPGA UART
//
IO_LOC "USB_TX" U15; IO_PORT "USB_TX" PULL_MODE=NONE IO_TYPE=LVCMOS15;
IO_LOC "USB_RX" V14; IO_PORT "USB_RX" PULL_MODE=NONE IO_TYPE=LVCMOS15;

//
// on board SDCard slot
//
IO_LOC "sd_clk"   V15; IO_PORT "sd_clk"  PULL_MODE=NONE IO_TYPE=LVCMOS15;
IO_LOC "sd_mosi"  Y16; IO_PORT "sd_mosi" PULL_MODE=NONE IO_TYPE=LVCMOS15;
IO_LOC "sd_miso" AA15; IO_PORT "sd_miso" PULL_MODE=NONE IO_TYPE=LVCMOS15;
IO_LOC "sd_cs_n"  W15; IO_PORT "sd_cs_n" PULL_MODE=NONE IO_TYPE=LVCMOS15;
// sd_det of TangConsole seems not working :-(
IO_LOC "sd_det_n" Y13; IO_PORT "sd_det_n" PULL_MODE=UP IO_TYPE=LVCMOS15;

